;1. Coherent scattering (barn/atom)
;2. Incoherent scattering (barn/atom)
;3. Photoelectric Absorption (barn/atom)
;4. Pair Production in nuclear field (barn/atom)
;5. Pair Production in electron field (barn/atom)

data=CALL_FUNCTION("XCOM"+ZSymb,ind,AtomicWeight=AtomicWeight,_EXTRA = ex)

; x barn/atom = x 10^(-24) cm^2/atom = x 10^(-24) (cm^2/atom) Na(atoms/mol)
;                    -----------------------------------
;                        MM (g/mol)
data[1:*,*]*=avogadroconstant(/noexp)/AtomicWeight

if n_elements(E) eq 0 then return,data

tmp = data[0,*]-E
ind1 = where(tmp le 0,ct1)
ind2 = where(tmp gt 0,ct2)
if (ct1 eq 0) and (ct2 ne 0) then return,data[1:*,0]
if (ct1 ne 0) and (ct2 eq 0) then return,data[1:*,(size(data,/dim))[1]-1]
if (ct1 ne 0) and (ct2 ne 0) then begin
    ind1 = ind1[ct1-1]
    ind2 = ind2[0]
    ret=XCOMInterpol(data[0,*],data[1,*],ind1,ind2,E,ind[1])
    for i=2,n_elements(ind)-1 do $
        ret=[ret,XCOMInterpol(data[0,*],data[i,*],ind1,ind2,E,ind[i])]
    return,ret
endif

return,0.
end;function XCOM
